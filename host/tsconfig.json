{
	"compilerOptions": {
		"target": "es6",
		"module": "esnext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"strict": true,
		"outDir": "out",
		"sourceMap": true,
		"esModuleInterop": true,
		"allowJs": true
	},
	"exclude": [
		"out",
		"node_modules"
	],
	"ts-node": {
        "compilerOptions": {
            "module": "CommonJS"
        }
    }
}